{
  "name": "registration-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "dev": "dotenv -e .env.local next dev",
    "build": "dotenv -e .env.production next build",
    "start": "dotenv -e .env.production next start",
    "lint": "next lint",
    "build:worker": "dotenv -e .env.production opennextjs-cloudflare",
    "dev:worker": "dotenv -e .env.local wrangler dev --port 8771",
    "preview:worker": "npm run build:worker && npm run dev:worker",
    "deploy:worker": "npm run build:worker && wrangler deploy"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.7.9",
    "next": "15.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^0.4.2",
    "@types/node": "^20",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.5",
    "npm-force-resolutions": "^0.0.10",
    "typescript": "^5"
  },
  "resolutions": {
    "esbuild": "^0.24.3"
  }
}
